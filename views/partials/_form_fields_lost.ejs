<label>Company Name:</label>
    <input type="text" name="companyName" id="company-name" autocomplete="off"  placeholder="e.g. Optimizely, Inc." value="<%= lostDeal.companyName %>">
<br>
<label>Closed Out On:</label>
    <input type="date" name="closedOn" id="closed-on" autocomplete="off" placeholder="e.g. 2/15/2020" value=""><!-- lostDeal.closedOn.toISOString().split('T')[0] -->
<br>
<label>Deal Type:</label>
    <select class="deal-type" name= "dealType" id="deal-type" autocomplete="off"  value="<%= lostDeal.dealType %>">
        <option value="New" name="dealType" >New</option>
        <option value="Horizontal Expansion" name="dealType" >Horizontal Expansion</option>
        <option value="Vertical Expansion" name="dealType" >Vertical Expansion</option>
    </select>
<br>
<label>Industry:</label>
    <select class= "industry" name= "industry" id="industry" autocomplete="off" value="<%= lostDeal.indusry %>">
        <option value="B2B Software" name="industry" >B2B Software</option>
        <option value="Retail/eCommerce" name="industry" >Retail/eCommerce</option>
        <option value="FinServ" name="industry" >FinServ</option>
        <option value="Media" name="industry" >Media</option>
        <option value="B2C Software" name="industry" >B2C Software</option>
        <option value="Travel" name="industry" >Travel</option>
        <option value="EdTech" name="industry" >EdTech</option>
        <option value="Other" name="industry" >Other</option>
    </select>
<br>
<label>ACV:</label>
    <input type="number" name="ACV" id="acv" placeholder="e.g. $100,000" autocomplete="off" value="<%= lostDeal.ACV %>" required>
<br>
<label>Date That The Customer First Engaged:</label>
    <input type="date" name="dateFirstEngaged" autocomplete="off" id="date-first-engaged" value="<%- lostDeal.dateFirstEngaged.toISOString().split('T')[0] %>">
<br>
<label>Executive Involved:</label>
    <select class= "executive-involved" name= "executiveInvolved" id="executive-involved" autocomplete="off" value="<%= lostDeal.executiveInvolved %>">
            <option value="Yes" name="executiveInvolved" selected>Yes</option>
            <option value="No" name="executiveInvolved">No</option>
    </select>
<br>
<label>Main Competitor:</label>
    <input type="text" name="mainCompetitor" id="main-competitor" placeholder="VWO" autocomplete="off" value="<%= lostDeal.mainCompetitor %>">
<br>