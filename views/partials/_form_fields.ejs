<label>Company Name:</label>
    <input type="text" name="companyName" id="company-name" placeholder="e.g. Optimizely, Inc." autocomplete="off" value="<%= closedDeal.companyName %>">
<br>
<label>Deal Type:</label>
    <select class="deal-type" name= "dealType" id="deal-type" autocomplete="off"  value="<%= closedDeal.dealType %>">
        <option value="New" name="dealType" >New</option>
        <option value="Horizontal Expansion" name="dealType" >Horizontal Expansion</option>
        <option value="Vertical Expansion" name="dealType" >Vertical Expansion</option>
    </select>
<br>
<label>Industry:</label>
    <select class= "industry" name= "industry" id="industry" autocomplete="off" value="<%= closedDeal.industry %>">
        <option value="B2B Software" name="industry" >B2B Software</option>
        <option value="Retail/eCommerce" name="industry" >Retail/eCommerce</option>
        <option value="FinServ" name="industry" >FinServ</option>
        <option value="Media" name="industry" >Media</option>
        <option value="B2C Software" name="industry" >B2C Software</option>
        <option value="Travel" name="industry" >Travel</option>
        <option value="EdTech" name="industry" >EdTech</option>
        <option value="Other" name="industry" >Other</option>
    </select>
<br>
<label>Closed On:</label>
    <input type="date" name="closedOn" id="closed-on" autocomplete="off" value="<%- closedDeal.closedOn.toISOString().split('T')[0] %>">
<br>
<label>Term Length in Years:</label>
    <select class= "current-term-length" name= "termLength" autocomplete="off"  id="current-term-length" value="<%= closedDeal.termLength %>">
        <option value="1" name="termLength" >One Year</option>
        <option value="2" name="termLength">Two Year</option>
        <option value="3" name="termLength">Three Year</option>
    </select>
<br>
<label>ACV:</label>
    <input type="number" name="ACV" id="acv" placeholder="e.g. $100,000" autocomplete="off"  value="<%= closedDeal.ACV %>">
<br>
<label>Revenue in Years 2+:</label>
    <input type="number" name="multiYearRevenue" id="current-multi-year-revenue" placeholder="e.g. $100,000" autocomplete="off"  value="<%= closedDeal.multiYearRevenue %>">
<br>
<label>Services Hours:</label>
    <input type="number" name="servicesHours" id="current-services-hours" placeholder="e.g. 80" autocomplete="off"  value="<%= closedDeal.servicesHours %>">
<br>
<label>Date That The Customer First Engaged:</label>
    <input type="date" name="dateFirstEngaged" autocomplete="off" id="date-first-engaged" value="<%- closedDeal.dateFirstEngaged.toISOString().split('T')[0] %>">
<br>
<label>Executive Involved:</label>
    <select class= "executive-involved" name= "executiveInvolved" id="executive-involved" autocomplete="off" value="<%= closedDeal.executiveInvolved %>">
            <option value="Yes" name="executiveInvolved" selected>Yes</option>
            <option value="No" name="executiveInvolved">No</option>
    </select>
<br>
<label>Main Competitor:</label>
    <input type="text" name="mainCompetitor" id="main-competitor" autocomplete="off"  placeholder="VWO" value="<%= closedDeal.mainCompetitor %>">
<br>